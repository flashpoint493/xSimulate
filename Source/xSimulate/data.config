<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <version>0.1</version>
  <name>test</name>
  <url>http://url.com</url>
  <steps>
    <step>
      <name>Step_Page</name>
      <actions>
        <action type="Page">
          <attributes>
            <attribute name="url">http://www.newegg.cn/Product/A36-050-C3L.htm</attribute>
          </attributes>
        </action>
        <action type="Click">
          <saveData>false</saveData>
          <attributes>
            <attribute name="id">quickBuy</attribute>
            <attribute name="click">true</attribute>
          </attributes>
        </action>
        <action type="Find">
          <context frame="0"></context>
          <attributes>
            <attribute name="id">name</attribute>
          </attributes>
        </action>
        <action type="SendKey">
          <attributes>
            <attribute name="sendkey_1">ipad</attribute>
            <attribute name="sendkey_2">{enter}</attribute>
          </attributes>
        </action>
      </actions>
    </step>
    
    <step enabled="false">
      <name>Step1</name>
      <actions>
        <action type="Page">
          <attributes>
            <attribute name="url">http://www.newegg.cn</attribute>
          </attributes>
        </action>
        <action type="Mouse">
          <attributes>
            <attribute name="id">topSearch</attribute>
            <attribute name="focus">true</attribute>
          </attributes>
        </action>
        <action type="SendKey" enabled="false">
          <attributes>
            <attribute name="id">topSearch</attribute>
            <attribute name="sendkey_1">ipad</attribute>
            <attribute name="sendkey_2">{enter}</attribute>
          </attributes>
        </action>
        <action type="Attribute" enabled="false">
          <attributes>
            <attribute name="id">topSearch</attribute>
            <attribute name="value">search</attribute>
          </attributes>
        </action>
        <action type="Find">
          <attributes>
            <attribute name="class">timelimit</attribute>
            <attribute name="wait">1000</attribute>
          </attributes>
          <childActions>
            <action type="Find">
              <attributes>
                <attribute name="combine">true</attribute>
                <attribute name="tagname">a</attribute>
                <attribute name="innertext">更多抢购商品</attribute>
                <attribute name="index">0</attribute>
              </attributes>
            </action>
          </childActions>
        </action>
        <action type="Click" enabled="false">
          <saveData>false</saveData>
          <attributes>
            <attribute name="click">true</attribute>
          </attributes>
        </action>
      </actions>
    </step>

    <step enabled="false">
      <name>Step2</name>
      <actions>
        <action type="Wait">
          <attributes>
            <attribute name="seconds">1</attribute>
          </attributes>
        </action>
        <action type="Find">
          <attributes>
            <attribute name="id">itemGrid1</attribute>
          </attributes>
          <childActions>
            <action type="Find">
              <attributes>
                <attribute name="combine">true</attribute>
                <attribute name="tagname">a</attribute>
                <attribute name="href">http://www.newegg.cn/Product/A1R-184-00K-02.htm</attribute>
                <attribute name="index">0</attribute>
              </attributes>
            </action>
          </childActions>
        </action>
        <action type="Click">
          <saveData>false</saveData>
          <attributes>
            <attribute name="click">true</attribute>
          </attributes>
        </action>
      </actions>
    </step>

    <step enabled="false">
      <name>Step2</name>
      <actions>
        <action type="Find">
          <attributes>
            <attribute name="id">quickBuy</attribute>
          </attributes>
        </action>
        <action type="Click">
          <saveData>false</saveData>
          <attributes>
            <attribute name="click">true</attribute>
          </attributes>
        </action>
      </actions>
    </step>
  </steps>
</config>