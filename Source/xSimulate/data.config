                <attribute name="href">http://www.newegg.cn/Product/A23-150-20X.htm?neg_sp=Home-_-A23-150-20X-_-CountdownV1</attribute>
                <attribute name="index">0</attribute>
              </attributes>
            </action>
            <action type="Click" savedata="false">
              <attributes>
                <attribute name="click">true</attribute>
              </attributes>
            </action>
          </childActions>
        </action>
      </actions>
    </step>
  </steps>
</config>